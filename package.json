{
  "name": "dds.wasm",
  "private": true,
  "version": "0.0.1",
  "main": "src/dds2img.js",
  "scripts": {
    "dev": "npx vite",
    "compile": "emcc -O3 -s WASM=1 -s EXPORT_ES6=1 -s MODULARIZE=1 -g0 -s EXTRA_EXPORTED_RUNTIME_METHODS='[\"cwrap\"]' -I dds/ src/wrapper.c  lib/dds/dds.c -o public/dds.js",
    "build": "npx vite build",
    "preview": "npx vite preview"
  },
  "devDependencies": {
    "eslint": "^8.28.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "vite": "^3.2.4",
    "vitest": "^0.25.3"
  }
}
